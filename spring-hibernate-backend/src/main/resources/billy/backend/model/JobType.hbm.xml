<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="billy.backend.model.JobType" table="job_type">

		<id name="id" type="long">
			<column name="job_type_id" />
			<generator class="sequence">
				<param name="sequence">job_type_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="namaJobType" type="string">
			<column name="nama_job_type" length="254" not-null="true" unique="true" />
		</property>	
		<property name="lastUpdate" type="timestamp">
			<column name="last_update" length="29" />
		</property>	
		<property name="updatedBy" type="string">
			<column name="updated_by" length="50" />
		</property>	
		
	</class>
</hibernate-mapping>
