<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="billy.backend.model.HargaBarang" table="harga_barang">

		<id name="id" type="long">
			<column name="harga_barang_id" />
			<generator class="sequence">
				<param name="sequence">harga_barang_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="intervalKredit" type="integer">
			<column name="interval_kredit" not-null="true" />
		</property>		
		<many-to-one name="Barang" class="billy.backend.model.Barang"
			fetch="join">
			<column name="barang_id" not-null="true" />
		</many-to-one>
		<property name="hargaBarang" type="big_decimal">
			<column name="harga_barang" precision="12" />
		</property>
		<property name="cicilanPerBulan" type="big_decimal">
			<column name="cicilan_per_bulan" precision="12" />
		</property>				
		<property name="komisiSales" type="big_decimal">
			<column name="komisi_sales" precision="12" />
		</property>
		<property name="tabunganSales" type="big_decimal">
			<column name="tabungan_sales" precision="12" />
		</property>
		<property name="lastUpdate" type="timestamp">
			<column name="last_update" length="29" />
		</property>	
		<property name="updatedBy" type="string">
			<column name="updated_by" length="50" />
		</property>	
		
	</class>
</hibernate-mapping>
