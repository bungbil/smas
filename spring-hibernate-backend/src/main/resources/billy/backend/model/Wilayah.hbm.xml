<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="billy.backend.model.Wilayah" table="Wilayah">

		<id name="id" type="long">
			<column name="wilayah_id" />
			<generator class="sequence">
				<param name="sequence">wilayah_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		<property name="kodeWilayah" type="string">
			<column name="kode_wilayah" length="50" not-null="true" unique="true" />
		</property>
		<property name="namaWilayah" type="string">
			<column name="nama_wilayah" length="254" />
		</property>
		<property name="status" type="string">
			<column name="status" length="50" />
		</property>		
		<property name="lastUpdate" type="timestamp">
			<column name="last_update" length="29" />
		</property>	
		<property name="updatedBy" type="string">
			<column name="updated_by" length="50" />
		</property>	
		
	</class>
</hibernate-mapping>
