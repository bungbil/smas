<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="billy.backend.model.KaryawanImages" table="karyawan_images">

		<id name="id" type="long">
			<column name="karyawan_images_id" />
			<generator class="sequence">
				<param name="sequence">karyawan_images_seq</param>
			</generator>
		</id>

		<version name="version" column="VERSION" type="integer" />

		
		<property name="profileImage" type="binary">
            <column name="profile_image" />
        </property>
        <property name="ktpImage" type="binary">
            <column name="ktp_image" />
        </property>
        
		<many-to-one name="karyawan" class="billy.backend.model.Karyawan"
			fetch="select" lazy="false">
			<column name="karyawan_id" />
		</many-to-one>
		
		
	</class>
</hibernate-mapping>
